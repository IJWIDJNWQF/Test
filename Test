local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Style Hub", "DarkTheme")

local Tab = Window:NewTab("Teleport")
local Section = Tab:NewSection("Teleport")

Section:NewButton("Teleport T1", "Teleport T1", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
end)

Section:NewButton("Teleport T2", "Teleport T2", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
end)

Section:NewButton("Teleport T3", "Teleport T3", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
end)

local Tab = Window:NewTab("Exit UI")
local Section = Tab:NewSection("Exit UI")

Section:NewKeybind("Exit UI", "Exit UI", Enum.KeyCode.G, function()
	Library:ToggleUI()
end)
